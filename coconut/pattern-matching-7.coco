def list_type(x):
    match x:
        case [a, b]:
            return "list, two items"
        case [a]:
            return "list, one item"
        case []:
            return "empty list"
        case _:
            return "list with more than 2 items"


print(list_type([]))
print(list_type([1]))
print(list_type([1, 2]))
print(list_type([1, 2, 3]))
